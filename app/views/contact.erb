            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12"> 
              <!-- Text input-->
              <div class="form-group">
                <label class=" control-label" for="name">Nome :</label>
                <input id="name" name="name" type="text" class="form-control" placeholder=" " required="">
